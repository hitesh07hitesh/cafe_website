<div class="container-fluid mt-5" id="Contact">
    <div class="row">
        <div class="col-md-10 col-12 mx-auto">
            <h2 class="d-flex justify-content-center mb-5 gfg border rounded-pill bg-success text-white">Contact us</h2>
            <div class="container-fluid mt-5">
                <div class="row">
                    <div class="col-md-6 col-12 mx-auto">
                        <div class="row">
                            <div class="mt-5 conatiner">
                                <form action="/contact" method="POST">
                                    <div class="text-center">
                                        <h3 class="text-success">How Can We Help You?</h3>
                                        <p class="lead">Submit your feedback / Contact Us </p>
                                    </div>
                                    <div>
                                        <label for="name" class="form-label">Your Name</label>
                                        <input type="text" name="name" class="form-control" placeholder="Your Name"
                                            required>
                                    </div>
                                    <div class="mt-3">
                                        <label for="email" class="form-label">Your Email</label>
                                        <input type="text" name="email" class="form-control" placeholder="Your Email"
                                            required>
                                    </div class="mt-3">
                                    <div class="mt-3">
                                        <label for="subject" class="form-label">Phone No.</label>
                                        <input type="number" name="phone" class="form-control" placeholder="Phone No."
                                            required>
                                    </div>
                                    <div class="mt-3 mb-3">
                                        <label for="message" class="form-label">Message</label>
                                        <textarea name="message" cols="20" rows="6" class="form-control"
                                            placeholder="message"></textarea>
                                    </div>
                                    <button type="submit" class="btn-style btn-primary btn btn-success">Submit</button>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-12  order-md-0 order-1 ">
                        <div class="map mt-5">
                            <iframe
                                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d448193.95106231474!2d76.76355628860217!3d28.644287345999494!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x390cfd5b347eb62d%3A0x37205b715389640!2sDelhi!5e0!3m2!1sen!2sin!4v1690039188407!5m2!1sen!2sin"
                                width="500" height="600" style="border:0;" allowfullscreen="" loading="lazy"
                                referrerpolicy="no-referrer-when-downgrade"></iframe>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

